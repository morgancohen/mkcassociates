	<sm id="SM-7F1B196C-CDA3-42ED-86D8-1ED3CF43F0BF">
		<smName>Number of Woodstoves</smName>
		<smIIID>II-3A5E4861-3639-4364-BB47-F963A95173EA</smIIID>
